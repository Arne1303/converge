{
    "name": "fluxtor/converge",
    "description": "static site generator for web artisans",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Fluxtor\\Converge\\": "src/"
        }
    },
    "authors": [
        {
            "name": "charrafi mohamed",
            "email": "charrafimohamed27@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "laravel/pint": "dev-main",
        "pestphp/pest": "3.x-dev",
        "orchestra/testbench": "9.x-dev",
        "phpstan/phpstan": "^2.1",
        "larastan/larastan": "3.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fluxtor\\Converge\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fluxtor\\Converge\\Providers\\ConvergeServiceProvider"
            ]
        }
    },
    "require": {
        "tempest/highlight": "^2.11"
    }
}
